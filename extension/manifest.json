{
  "manifest_version": 3,
  "name": "SEO Meta Analyzer",
  "version": "1.1.0",
  "description": "Analyzes title, meta description, and tracking scripts on web pages.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
